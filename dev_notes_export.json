      "controls": [
        {
          "fieldId": "title",
          "settings": {
            "helpText": "The name of your new theme!"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "primary",
          "settings": {
            "helpText": "Required as the majority of other colors will calculate relative to this color"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "secondary",
          "settings": {
            "helpText": "Used in conjunction with Primary to separate sections. Will calculate 7% darker than Primary if no value entered."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "background",
          "settings": {
            "helpText": "If left empty, will calculate a color 15% darker than Primary"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "highlight",
          "settings": {
            "helpText": "Generally for buttons and some headers."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "highlightHover",
          "settings": {
            "helpText": "This is 10% darker than Highlight color which will usually work great."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "link",
          "settings": {
            "helpText": "Used to give link text a bit more legibility separate from highlight if that's an issue."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "linkHover",
          "settings": {
            "helpText": "!Rarely needed! This is 10% darker than Link color which will usually work."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "white",
          "settings": {
            "helpText": "Pick a color that will be visibly clear to read against your Primary color"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "gray",
          "settings": {
            "helpText": "Required, but you can typically leave this as its default"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "error",
          "settings": {
            "helpText": "Required, but you can typically leave this as its default"
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "codeLine",
          "settings": {
            "helpText": "The background for single lines of <code />. You can probably leave this blank as its relative color will generally look good on its own."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "codeBlock",
          "settings": {
            "helpText": "For multi-line blocks of <code />. You can probably leave this blank as its relative color will generally look good on its own."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "placeholder",
          "settings": {
            "helpText": " Used for improving legibility of the Gray color for placeholder text. This is 10% lighter than Gray/Inactive color which will usually work."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        },
        {
          "fieldId": "codeText",
          "settings": {
            "helpText": "!Rarely needed! This is a fallback color incase the regular Text / White color does not mesh with the code block/code line colors."
          },
          "widgetId": "singleLine",
          "widgetNamespace": "builtin"
        }
      ]
    }
